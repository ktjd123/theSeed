{
  "name": "nextjs-express-mongoose-crudify-boilerplate",
  "version": "3.0.0",
  "description": "Next.js with Express boilerplate and Mongoose CRUD support",
  "main": "server/index.js",
  "scripts": {
    "d": "rm -rf build && cross-env NODE_ENV=development tsc-watch -p tsconfig.server.json --onSuccess \"node build/index.js\"",
    "p": "cross-env NODE_ENV=production node index.js",
    "build": "rm -rf .next && rm -rf build && tsc -p tsconfig.server.json && next build && mv .next build/.next && cp package.json build && cp -r public build/public && dploy stage && yarn ssh",
    "start": "cd build && NODE_ENV=production node index.js",
    "ssh": "ssh ubuntu@ -i ~/Documents/cert/Dekina.pem"
  },
