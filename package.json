{
  "name": "dekina-accounts",
  "version": "1.0.0",
  "description": "Dekina accounts service",
  "license": "UNLICENSED",
  "main": "server/index.js",
  "scripts": {
    "build": "rm -rf .next && rm -rf build && tsc -p tsconfig.server.json && next build && mv .next build/.next && cp package.json build && cp -r public build/public && dploy stage && yarn ssh",
    "start": "rm -rf build && cross-env NODE_ENV=development tsc-watch -p tsconfig.server.json --onSuccess \"node build/index.js\"",
    "p": "cross-env NODE_ENV=production node index.js",
    "ssh": "ssh ubuntu@ -i ~/Documents/cert/Dekina.pem",
    "build-start": "cd build && node index.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "connect-mongo": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "include-media": "^1.4.10",
    "joi": "^17.4.1",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "mongoose": "^5.13.3",
    "morgan": "^1.10.0",
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-toastify": "^7.0.4",
    "sass": "^1.36.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.1",
    "@types/compression": "^1.7.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.3",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
